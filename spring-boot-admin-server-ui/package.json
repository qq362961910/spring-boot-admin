{
  "name": "spring-boot-admin-server-ui",
  "description": "Spring Boot Admin UI",
  "private": true,
  "scripts": {
    "watch": "vue-cli-service build --watch  --mode development",
    "build": "vue-cli-service build  --mode development",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.15",
    "@fortawesome/free-brands-svg-icons": "^5.7.2",
    "@fortawesome/free-regular-svg-icons": "^5.7.2",
    "@fortawesome/free-solid-svg-icons": "^5.7.2",
    "@fortawesome/vue-fontawesome": "^0.1.5",
    "ansi_up": "^4.0.3",
    "autolinker": "^3.0.4",
    "axios": "^0.18.0",
    "bulma": "^0.7.4",
    "bulma-badge": "^3.0.0",
    "d3-array": "^2.0.3",
    "d3-axis": "^1.0.12",
    "d3-brush": "^1.0.6",
    "d3-scale": "^2.2.2",
    "d3-selection": "^1.4.0",
    "d3-shape": "^1.3.4",
    "d3-time": "^1.0.11",
    "event-source-polyfill": "^1.0.5",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "moment-shortformat": "^2.1.0",
    "popper.js": "^1.14.7",
    "pretty-bytes": "^5.1.0",
    "resize-observer-polyfill": "^1.5.1",
    "rxjs": "^6.4.0",
    "vue": "^2.6.8",
    "vue-clickaway2": "^2.3.1",
    "vue-router": "^3.0.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.5.1",
    "@vue/cli-plugin-eslint": "^3.5.1",
    "@vue/cli-plugin-unit-jest": "^3.5.3",
    "@vue/cli-service": "^3.5.3",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "eslint": "^5.15.1",
    "eslint-plugin-vue": "^5.2.2",
    "html-loader": "^0.5.5",
    "jest": "^24.5.0",
    "node-sass": "^4.11.0",
    "sass-loader": "^7.0.1",
    "vue-template-compiler": "^2.6.8",
    "webpack-bundle-analyzer": "^3.1.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:vue/strongly-recommended"
    ],
    "rules": {
      "quotes": [
        "error",
        "single"
      ],
      "no-console": [
        "error",
        {
          "allow": [
            "warn"
          ]
        }
      ],
      "vue/max-attributes-per-line": "off",
      "vue/component-name-in-template-casing": "kebab-case"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "overrides": [
      {
        "files": [
          "*.spec.js"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "jest": {
    "setupFiles": [
      "<rootDir>/src/test/frontend/globals.js"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      ".+\\.(css|styl|less|sass|scss|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.js$": "babel-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/main/frontend/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "**/*.spec.js"
    ],
    "testURL": "http://example.com"
  }
}
